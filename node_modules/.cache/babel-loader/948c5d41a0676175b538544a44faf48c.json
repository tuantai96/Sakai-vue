      <Column field=\"inventoryStatus\" header=\"Trạng thái\" :sortable=\"true\" headerStyle=\"min-width:10rem;\">\r\n          <template #body=\"slotProps\">\r\n            <span class=\"p-column-title\">Status</span>\r\n            <span :class=\"'product-badge status-' + (slotProps.data.inventoryStatus ? slotProps.data.inventoryStatus.toLowerCase() : '')\">{{slotProps.data.inventoryStatus}}</span>\r\n          </template>\r\n        </Column>\r\n      </DataTable>\r\n\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n<script>\r\nimport {useI18n} from 'vue-i18n';\r\nexport default {\r\n  setup() {\r\n    const {t} = useI18n();\r\n    return {t}\r\n  },\r\n  data() {\r\n    return {\r\n      statusOptions: [\r\n        {status: 'Đang hoạt động', code: '1'},\r\n        {status: 'Không hoạt động', code: '2'},\r\n      ],\r\n      roleOptions: [\r\n        {roleType: 'Mặc định', code: '1'},\r\n        {roleType: 'Tổ chức', code: '2'}\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.$router.push({\r\n        name: 'CreateRole'\r\n      })\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n</style>"]},"metadata":{},"sourceType":"module"}